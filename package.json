    "@rollup/plugin-node-resolve": "^13.1.3",
    "postcss": "^8.4.6",
    "rollup": "^2.68.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.31",
    "vue-template-compiler": "^2.6.14"
